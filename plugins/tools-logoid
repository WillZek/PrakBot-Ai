import fetch from 'node-fetch';
const handler = async (m, {conn, text, usedPrefix, command}) => {
if (!text) throw `*ğŸ’› ingrese una peticiÃ³n para generar un logo...*`;
m.react('ğŸ•’');
await conn.sendMessage(m.chat, {text: '*ğŸ’› Espere un momento...*'}, {quoted: m});
try {
const response = await fetch(`https://api.ideogram.ai/generate?text=${encodeURIComponent(text)}`);
if (!response.ok) throw new Error('Network response was not ok');
const json = await response.json();
const logoUrl = json.logoUrl; // AsegÃºrate de que esta propiedad sea la correcta segÃºn la respuesta de la API
m.react('â˜‘ï¸');
await conn.sendMessage(m.chat, {image: {url: logoUrl}}, {quoted: m});
} catch (error) {
console.error(error);
throw `Error...`;
}
}
handler.tags = ['tools']
handler.help = ['logoid']
handler.command = ['logoid'];
export default handler;